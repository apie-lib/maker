{
  "name": "apie/maker",
  "description": "Composer package of the apie library: maker",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Pieter Jordaan",
      "email": "pieter_jordaan@hotmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Apie\\Maker\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Apie\\Tests\\Maker\\": "tests/"
    }
  },
  "extra": {
    "apie-objects": [
      "Apie\\Maker\\Dtos\\DomainObjectDto",
      "Apie\\Maker\\Enums\\IdType"
    ]
  },
  "require": {
    "php": ">=8.1",
    "apie/common": "self.version",
    "apie/core": "self.version",
    "haydenpierce/class-finder": "0.5.3",
    "nette/php-generator": "^4.0.1",
    "nikic/php-parser": "^4.14"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "phpspec/prophecy-phpunit": "^2.0"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}